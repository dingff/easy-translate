{
  "action": {
    "default_icon": {
      "128": "./logo/logo.png",
      "16": "./logo/logo.png",
      "32": "./logo/logo.png",
      "48": "./logo/logo.png"
    },
    "default_popup": "./popup.html"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "css": ["./content.css"],
      "js": ["./js/common.js", "./js/md5.js", "./content.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "background": {
    "service_worker": "./background.js"
  },
  "description": "本工具是一款智能感知的瞬时交互翻译工具, 鼠标移入即可翻译",
  "icons": {
    "128": "./logo/logo.png",
    "16": "./logo/logo.png",
    "32": "./logo/logo.png",
    "48": "./logo/logo.png"
  },
  "manifest_version": 3,
  "name": "Easy Translate",
  "permissions": ["storage"],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.0.1"
}
